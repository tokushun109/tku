{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "packageManager": "pnpm@9.15.9",
    "lint-staged": {
        "**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
            "pnpm fmt"
        ]
    },
    "scripts": {
        "dev": "next dev",
        "dev:3001": "cross-env PORT=3001 next dev -p 3001 ",
        "build": "next build",
        "start": "next start",
        "lint": "eslint --max-warnings=0 .",
        "lint-fix": "eslint --fix .",
        "fmt": "prettier --write .",
        "type-check": "next typegen && tsc --noEmit",
        "sb": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "test": "vitest --project=unit",
        "test-storybook": "pnpm build-storybook --quiet && vitest --project=storybook",
        "test-integration": "vitest --project=integration",
        "test-all": "vitest",
        "test-coverage": "vitest --coverage",
        "prepare": "cd .. && husky frontend/.husky"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@fontsource/roboto": "^5.2.6",
        "@hookform/resolvers": "^5.2.1",
        "@mui/icons-material": "^7.3.2",
        "@mui/material": "^7.3.2",
        "@next/third-parties": "^15.5.2",
        "@storybook/addon-styling-webpack": "^2.0.0",
        "@storybook/addon-themes": "^9.1.4",
        "@types/react-transition-group": "^4.4.12",
        "classnames": "^2.5.1",
        "framer-motion": "^12.23.12",
        "http-status-codes": "^2.3.0",
        "next": "15.5.7",
        "react": "^19.1.1",
        "react-confetti": "^6.4.0",
        "react-dom": "^19.1.1",
        "react-hook-form": "^7.62.0",
        "react-transition-group": "^4.4.5",
        "react-virtuoso": "^4.14.0",
        "sonner": "^2.0.7",
        "zod": "^4.1.5"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "^4.1.1",
        "@eslint/compat": "^1.3.2",
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.34.0",
        "@mdx-js/react": "^3.1.1",
        "@next/eslint-plugin-next": "^15.5.2",
        "@storybook/addon-a11y": "^9.1.4",
        "@storybook/addon-controls": "^9.0.8",
        "@storybook/addon-docs": "^9.1.4",
        "@storybook/addon-links": "^9.1.4",
        "@storybook/addon-onboarding": "^9.1.4",
        "@storybook/addon-vitest": "^9.1.4",
        "@storybook/nextjs-vite": "^9.1.4",
        "@storybook/react": "^9.1.4",
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "^16.3.0",
        "@types/node": "^24.3.1",
        "@types/react": "^19.1.12",
        "@types/react-dom": "^19.1.9",
        "@vitest/browser": "^3.2.4",
        "@vitest/coverage-v8": "^3.2.4",
        "chromatic": "^13.1.4",
        "cross-env": "^10.0.0",
        "eslint": "^9.34.0",
        "eslint-config-next": "15.5.7",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-perfectionist": "^4.15.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-storybook": "^9.1.4",
        "eslint-plugin-unused-imports": "^4.2.0",
        "husky": "^9.1.7",
        "jsdom": "^26.1.0",
        "lint-staged": "^16.1.6",
        "markdown-to-jsx": "^7.7.13",
        "msw": "^2.11.1",
        "playwright": "^1.56.1",
        "prettier": "^3.6.2",
        "ress": "^5.0.2",
        "sass": "^1.92.0",
        "sass-loader": "^16.0.5",
        "storybook": "^9.1.4",
        "typescript": "^5.9.2",
        "vitest": "^3.2.4"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "core-js-pure",
            "esbuild",
            "msw",
            "sharp",
            "unrs-resolver"
        ]
    }
}
